<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
              http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	version="2.5" xmlns="http://java.sun.com/xml/ns/javaee">



	<!-- DB service -->
	<servlet>
		<servlet-name>DBService</servlet-name>
		<servlet-class>ro.fagadar.gwt.server.DBServiceImpl</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>DBService</servlet-name>
		<url-pattern>/gwt-sql/DBService</url-pattern>
	</servlet-mapping>


	<!-- DownloadHandler -->
	<servlet>
		<servlet-name>DownloadHandlerServlet</servlet-name>
		<servlet-class>ro.fagadar.gwt.server.DownloadHandler</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>DownloadHandlerServlet</servlet-name>
		<url-pattern>/gwt-sql/DownloadHandlerServlet</url-pattern>
	</servlet-mapping>



</web-app>
